@model List<LaunchCodeCapstone.Models.BlogStyleReview.Review>
﻿@using Microsoft.AspNetCore.Identity

@inject SignInManager<IdentityUser> signInManager

@{
    ViewData["Title"] = "Home Page";
}

<div class="container col-xxl-8 px-4 py-5">
    <div class="row align-tems-center g-5 py-5">
        <div class="col-12 col-lg-6">
            <h1 class="display-5 fw-bold lh-1 mb-3">
                Frame Rate
            </h1>
            <p class="lead">
                Frame Rate is your hotspot for rating, reviewing, and discovering the latest movies.
            </p>
        </div>


        <div class="col-12 col-lg-6">
            <img src="data:image/jpeg;base64,"
                 alt="filmReelQuoteLogo"
                 class="d-block mx-lg-auto img-fluid" width:300px; />
        </div>
    </div>
</div>

<div class="container">
    <div class="row justify-content-center">
        <div class="col-6">
            @if (Model != null && Model.Any())
            {
                @if (signInManager.IsSignedIn(User))
                {
                    foreach (var review in Model)
                    {
                        <div class="mb-5 bg-light box-shadow">
                            <img src="@review.FeaturedImageUrl"
                                 alt="@review.Heading"
                                 class="mb-2 d-block img-fluid">

                            <h2 class="mb-2">@review.Heading</h2>
                            <p class="mb-4">
                                Author: @review.Author
                                <br />
                                Published Date: @review.PublishedDate.ToShortDateString()
                            </p>
                            <p class="mb-4">
                                @review.Description
                            </p>
                            <a class="btn btn-dark"
                                asp-area=""
                                asp-controller="DisplayReviews"
                                asp-action="Index"
                                asp-route-urlHandle="@review.UrlHandle">Read More</a>
                        </div>
                    }
                }
            }
        </div>
    </div>
</div>